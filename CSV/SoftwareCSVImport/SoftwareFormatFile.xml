<CSVImportFormat>
  <Projection Type="DeviceProjection">
    <Seed>
      <Class Type="Microsoft.Windows.Computer">
        <Property ID="PrincipalName" />
      </Class>
    </Seed>
    <Component Alias="SoftwareInstalled" Count="1">
      <Seed>
        <Class Type="System.SoftwareItem">
			<Property ID="VersionString" />
			<Property ID="Publisher" />	
			<Property ID="ProductName" />
			<Property ID="IsVirtualApplication" />
			<Property ID="LocaleID" />
			<Property ID="MajorVersion" />
			<Property ID="MinorVersion" />
        </Class>
      </Seed>
    </Component>
    <Component Alias="SoftwareUpdate" Count="1">
      <Seed>
        <Class Type="Microsoft.Windows.SoftwareUpdate">
          <Property ID="Title" />
          <Property ID="Vendor" />
        </Class>
      </Seed>
    </Component>
  </Projection>
</CSVImportFormat>