<CSVImportFormat>
  <Projection Type="Microsoft.Windows.Computer.ProjectionType">
    <Seed>
      <Class Type="Microsoft.Windows.Computer">
        <Property ID="PrincipalName"/>
      </Class>
    </Seed>
    <Component Alias="LogicalDisk">
      <Seed>
        <Class Type="Microsoft.Windows.Peripheral.LogicalDisk">
          <Property ID="DisplayName"/>
          <Property ID="DeviceID"/>
          <Property ID="Name"/>
          <Property ID="Description"/>
          <Property ID="AssetStatus"/>
          <Property ID="VolumeName"/>
          <Property ID="FileSystem"/>
          <Property ID="Compressed"/>
          <Property ID="Size"/>
          <Property ID="FreeSpace"/>
          <Property ID="DriveType"/>
          <Property ID="SupportsDiskQuota"/>
          <Property ID="QuotasDisabled"/>
          <Property ID="SupportsFileBasedCompression"/>
        </Class>
      </Seed>
    </Component>
  </Projection>
</CSVImportFormat>
