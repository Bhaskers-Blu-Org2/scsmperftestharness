<CSVImportFormat>
  <Projection Type="Microsoft.Windows.Computer.ProjectionType">
    <Seed>
      <Class Type="Microsoft.Windows.Computer">
        <Property ID="PrincipalName"/>
      </Class>
    </Seed>
    <Component Alias="NetworkAdapter">
      <Seed>
        <Class Type="Microsoft.Windows.Peripheral.NetworkAdapter">
          <Property ID="DisplayName"/>
          <Property ID="DeviceID"/>
          <Property ID="Name"/>
          <Property ID="Description"/>
          <Property ID="AssetStatus"/>
          <Property ID="Notes"/>
          <Property ID="AdapterType"/>
          <Property ID="Index"/>
          <Property ID="Manufacturer"/>
          <Property ID="MACAddress"/>
          <Property ID="ServiceName"/>
          <Property ID="PerfmonInstance"/>
          <Property ID="DHCPEnabled"/>
          <Property ID="DHCPServer"/>
          <Property ID="DNSDomain"/>
          <Property ID="IPAddress"/>
          <Property ID="IPSubnet"/>
          <Property ID="Bandwidth"/>
          <Property ID="MaxSpeed"/>
          <Property ID="ProductName"/>
          <Property ID="DefaultIPGateway"/>
          <Property ID="DHCPHostname"/>
          <Property ID="IPEnabled"/>
        </Class>
      </Seed>
    </Component>
  </Projection>
</CSVImportFormat>

