<CSVImportFormat>
  <Projection Type="Microsoft.Windows.Computer.ProjectionType">
    <Seed>
      <Class Type="Microsoft.Windows.Computer">
        <Property ID="PrincipalName"/>
      </Class>
    </Seed>
    <Component Alias="PhysicalDisk">
      <Seed>
        <Class Type="Microsoft.Windows.Peripheral.PhysicalDisk">
          <Property ID="DisplayName"/>
          <Property ID="DeviceID"/>
          <Property ID="Name"/>
          <Property ID="Description"/>
          <Property ID="AssetStatus"/>
          <Property ID="Notes"/>
          <Property ID="Caption"/>
          <Property ID="Index"/>
          <Property ID="InterfaceType"/>
          <Property ID="Manufacturer"/>
          <Property ID="Model"/>
          <Property ID="SCSIBus"/>
          <Property ID="SCSILogicalUnit"/>
          <Property ID="SCSIPort"/>
          <Property ID="SCSITargetID"/>
          <Property ID="Size"/>
          <Property ID="TotalCylinders"/>
          <Property ID="TotalHeads"/>
          <Property ID="TotalSectors"/>
          <Property ID="TotalTracks"/>
          <Property ID="TracksPerCylinder"/>
          <Property ID="PerfmonInstance"/>
          <Property ID="MediaType"/>
          <Property ID="PNPDeviceId"/>
        </Class>
      </Seed>
    </Component>
  </Projection>
</CSVImportFormat>
